package wiprotraining;

public class variabledemo {

    // Static variable
    static int num1 = 10;

    // Instance variable
    int num2 = 20;

    public void addTwoNumbers() {
  
        int sum = num1 + num2;

        System.out.println("Local Variable: " + sum);
    }

    public static void main(String[] args) {
        addTwoNumbers();
    }
}
